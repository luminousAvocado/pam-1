<div class="progressbar-container">
  <ul class="progressbar">
    <li class="step" data-title="Requester Information">
      <a class="d-none d-md-inline" asp-action="RequesterInfo" asp-route-id="@ViewData["id"]">Requester Information</a>
    </li>
    <li class="step" data-title="Transfer From Unit">
      <a class="d-none d-md-inline" asp-action="TransferFromUnit" asp-route-id="@ViewData["id"]">Transfer From</a>
    </li>
    <li class="step" data-title="Transfer To Unit">
      <a class="d-none d-md-inline" asp-action="TransferToUnit" asp-route-id="@ViewData["id"]">Transfer To</a>
    </li>
    <li class="step" data-title="Additional Information">
      <a class="d-none d-md-inline" asp-action="AdditionalInfo" asp-route-id="@ViewData["id"]">Additional Information</a>
    </li>
    <li class="step" data-title="Signatures">
      <a class="d-none d-md-inline" asp-action="Signatures" asp-route-id="@ViewData["id"]">Signature(s) Required</a>
    </li>
    <li class="step" data-title="Summary">
      <a class="d-none d-md-inline" asp-action="Summary" asp-route-id="@ViewData["id"]">Summary</a>
    </li>
  </ul>
</div>
